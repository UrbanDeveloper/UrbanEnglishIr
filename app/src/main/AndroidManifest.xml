<manifest xmlns:android="http://schemas.android.com/apk/res/android"

          package="com.example.ashamsa.recyclerviewtemplate"
    >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.ashamsa.UrbanEnglishIr.Level1_fragment_caller">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.ashamsa.UrbanEnglishIr.Level2_fragment_caller"
            android:parentActivityName="com.example.ashamsa.UrbanEnglishIr.Level1_fragment_caller">
        </activity>

        <activity
            android:name="com.example.ashamsa.UrbanEnglishIr.Level3_fragment_caller_pager"
            >
        </activity>

        <activity android:name="com.example.ashamsa.UrbanEnglishIr.Level3_userFeed_fragment_caller"
            >
        </activity>
    </application>

</manifest>
